<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="shortcut icon" type="image/png" href="/images/logoweather.png" />

  <link rel="stylesheet" href="/css/style.css">
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900&display=swap"
    rel="stylesheet">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="css/animate.css">
  <link rel="stylesheet" href="css/owl.carousel.min.css">
  <link rel="stylesheet" href="css/owl.theme.default.min.css">
  <link rel="stylesheet" href="css/magnific-popup.css">
  <link rel="stylesheet" href="css/ionicons.min.css">
  <link rel="stylesheet" href="css/flaticon.css">
  <link rel="stylesheet" href="css/icomoon.css">
  <link rel="stylesheet" href="css/style.css">
  <title>Weather App</title>
</head>

<body>
  {{>header}}
  <main>
    <section class="ftco-section ftco-no-pt ftco-no-pb">
      <div class="container">
        <div class="row py-5">
          <div class="col heading-section ftco-animate ">
            <!-- title -->
            <span class="subheading">Weather App Result</span>
            <div class="heading-section ftco-animate ">
              <h2 class="mb-4">Are you looking for?</h2>
              <p style="text-align: justify;">Please select the location closest to the results you want to search the
                most.
              </p>
            </div>
            {{#each dataAutocomplete}}
            {{>autocomplete_result}}
            <p>{{log this}}</p>
            {{/each}}
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section bg-primary ftco-no-pt ftco-no-pb">
      <div class="container">
        <div class="row d-flex no-gutters">
          <div class="col-md-12">
            <div class="row justify-content-start py-5">
              <div class="col-md-12 heading-section ftco-animate pr-md-5 py-md-4">
                <div class="heading-section ftco-animate ">
                  <h2 class="mb-4">Find other weather results</h2>
                </div>
                <div class="weather-search  ftco-animate">
                  <form name="weather_input" action="/autocomplete" method="POST" onsubmit="return validateForm()">
                    <div class="input-group mb-3">
                      <input id="search" type="text" class="form-control" placeholder="Type your location..."
                        name="search">
                      <div class="input-group-append">
                        <button class="btn btn-secondary" name="submit" value="Submit">Search</button>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </section>
  </main>
  {{>footer}}
</body>
<script src="js/jquery.min.js"></script>
<script src="js/jquery-migrate-3.0.1.min.js"></script>
<script src="js/popper.min.js"></script>
<script src="js/bootstrap.min.js"></script>
<script src="js/jquery.easing.1.3.js"></script>
<script src="js/jquery.waypoints.min.js"></script>
<script src="js/jquery.stellar.min.js"></script>
<script src="js/jquery.animateNumber.min.js"></script>
<script src="js/owl.carousel.min.js"></script>
<script src="js/jquery.magnific-popup.min.js"></script>
<script src="js/scrollax.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
<script src="js/google-map.js"></script>
<script src="js/main.js"></script>

</script>