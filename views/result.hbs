</head>

<body>
  {{!-- partials --}}
  {{>header}}
  <main>
    <section class="ftco-section">
      <div class="container">
        <div class="row">
          <div class="col">
            <div class="weather-wrapper">
              <div class="weather-card madrid" style="background-image:url('/images/weatherbg/{{data.icon}}.jpg')">
                <div class="weather-icon">
                  <img class="icon-style" src="/icon_weather/{{data.icon}}.svg">
                </div>
                <h1>{{data.temperature}}ยบ</h1>
                <div class="weather-detail">
                  <p>{{place_name}}</p>
                </div>
              </div>
            </div>
            <div class="weather-btn">
              <a href="/weather">More Locations</a>
            </div>
            <div class="weather-btn">

            </div>
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section testimony-section bg-primary">
      <div class="container-fluid">
        <div class="row justify-content-center mb-5">
          <div class="col-md-7 text-center heading-section heading-section-white ftco-animate">
            <span class="subheading">VIEW MORE DEATAIL WEATHER THIS LOCATION</span>
            <h2 class="mb-4">{{place_name}}</h2>
          </div>
        </div>
        <div class="row ftco-animate">
          <div class="col-md-12">
            <div class="carousel-testimony owl-carousel ftco-owl">
              {{#each data.hourly_data}}
              {{>list_hourly_result}}
              {{log this}}
              {{/each}}
            </div>
          </div>
        </div>
      </div>
    </section>
  </main>
  {{>footer}}
